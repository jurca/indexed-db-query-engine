{
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./dist/tsBuildInfoFile.json",

    "target": "ESNext",

    "module": "ESNext",
    "rootDir": "./",

    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist/",
    "sourceRoot": "../",
    "preserveConstEnums": true,

    "forceConsistentCasingInFileNames": true,

    "strict": true,

    "skipLibCheck": true
  }
}
